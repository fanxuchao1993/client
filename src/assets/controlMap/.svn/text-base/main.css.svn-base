@charset "utf-8";
/*body { background:#9dabba;}*/
.body-bg { background:url(../imgs/main/background/bg02container.png) no-repeat #9dabba;}
/* Login */
#login-wrap-bg {background:url("../imgs/login/background/bg01login.jpg") center top no-repeat; }
#login-wrap{ position:relative;}
#login-wrap div { position:absolute; left:50%; top:214px; margin-left:-302px; width:605px; height:393px; background:url("../imgs/login/background/bg02login.png") center top no-repeat;}
#login-wrap h1 { position:absolute; left:23px; top:23px; width:171px; height:70px; display:block; background:url("../imgs/login/block/tit01.png") no-repeat; text-indent:-5000px;}
#login-wrap h2 { position:absolute; left:50%; top:220px; margin-left:-227px; width:455px; height:64px; display:block; background:url("../imgs/login/block/tit02.png") no-repeat; text-indent:-5000px;}
#login-wrap p { margin-left:170px;}
#login-wrap dl { float:left; width:234px; margin-right:14px; margin-left:100px; margin: 155px 14px 30px 150px;}
#login-wrap dl dt { float:left; width:64px; padding-top:5px; margin-bottom:14px;}
#login-wrap dl dd { float:left; margin-bottom:14px;}
#login-wrap dl dd input{ border:0; margin:0; padding:0; padding-left:7px; width:160px; height:33px; line-height:33px; background:url("../imgs/login/background/bg01input.png") no-repeat;}
#login-wrap .submit { margin-top:155px;}

/* Main */

/*Header */
#header { position:relative; height: 120px; background:url("../imgs/main/background/bg01header.png") no-repeat;}
#header h1 { padding:23px 0 0 23px; height:66px;}
#header .Rbanner { position:absolute; right:0; top:0; width:180px; height:90px; background:url("../imgs/main/background/bg01header_r.png") no-repeat; }

/* Header Nav */
#header #nav { position:relative; height:33px; line-height:33px; padding: 0 0 0 10px; background:url("../imgs/main/background/bg01nav.png") repeat-x;box-shadow: 0px 3px 3px #8b99a9;}
#header #nav.nav1024 ul li a {padding: 0 2px!important; color:#fff; letter-spacing:-1px;}
#header #nav ul li { display:inline-block; *+float:left;}
#header #nav ul li a { display:block; padding: 0 12px; color:#fff; font-weight:bold; text-shadow:1px 1px 4px #666; text-decoration:none;}
#header #nav ul li a:hover { background:url("../imgs/main/background/bg02nav.png") center bottom no-repeat; text-shadow:1px 1px 4px #57d1ed;}
/* Header LocalTime */
#header .localTime_outer {position:absolute; right:10px; top:0;}
#header .localTime_outer p { height:20px; float:left; margin-left:10px; color:#FF0;}

/* Header Adminbox */
#header #adminbox { position:absolute; right:10px; bottom:32px; height:24px; color:#fff;}
#header #adminbox a { color:#fff;}

#header #adminbox .namebox { float:left; position:relative; width:210px; height:74px; background:url(../imgs/main/background/bg01admin.png) left top no-repeat;}
#header #adminbox .namebox div img { position:absolute; left:59px; top:13px;}
#header #adminbox .namebox p { position:absolute; right:20px; top:16px; text-align:left;  color:#fff; line-height:1.8;}
#header #adminbox ul { float:left; padding-left:20px; *+overflow:hidden;}
#header #adminbox ul li { margin:12px 0 0 0; display:inline-block; *+float:left; text-align:center;}
#header #adminbox ul li a { display:block;*+float:left; width:80px; text-align:center; color:#fff; text-decoration:none;}
#header #adminbox ul li a:hover { display:block;*+float:left; width:80px; background:url("../imgs/main/background/bg02nav.png") center bottom no-repeat;}
#header #adminbox ul li img { display:block; margin:0 auto; }

/* Container */
#nav-location {display:none!important;}

/* Container Left */
#left { float:left; width:81%; margin-top:10px; -moz-border-radius: 0 6px 6px 0; -webkit-border-radius: 0 6px 6px 0; border-radius: 0 6px 6px 0;}
.inter01L-01 { margin-left:10px; margin-bottom:0; min-height:310px;}

/* Container  Right*/
#right { float:left; width:18.2%; *+width:17%; margin-top:10px; margin-left:0.8%; -moz-border-radius:4px 0 4px 0; -webkit-border-radius:4px 0 4px 0; border-radius:4px 0 4px 0;}
.outer01-right h2 { position:relative;}
.outer01-right h2 span { position:absolute; right:5px; top:5px;}
.inter01R-01 { min-height:280px; -moz-border-radius:4px 0 4px 0; -webkit-border-radius:4px 0 4px 0; border-radius:4px 0 4px 0;}
.inter02R-01 { overflow:hidden;  text-align:center; min-height:207px;*+padding-left:40px ;}
.inter02R-01 ul { display:inline-block; *+float:left;}
.inter02R-01 ul li { width:112px; height:52px; margin:20px 10px 0 10px; line-height:2.2;}
.inter02R-01 ul.list01 li { background:url("../imgs/main/background/bg01kbox.png") no-repeat;}
.inter02R-01 ul.list02 li { background:url("../imgs/main/background/bg02kbox.png") no-repeat;}
.inter02R-01 ul.list03 li { background:url("../imgs/main/background/bg03kbox.png") no-repeat;}
.inter02R-01 ul li strong, .inter02R-01 ul li b { display:block;}
.inter02R-01 ul li strong { color:#fff;}
.inter02R-01 ul li b { color:#000;}
.inter03R-01 ul { margin:10px 0 0 20px;}
.inter03R-01 ul li { line-height:2; position:relative; padding-left:14px; background:url("../imgs/main/block/list01txt.png") left center no-repeat;}
.inter03R-01 ul li em { position:absolute; right:40px; top:0;*+top:-8px; color:#1f62ab;}
.inter03R-01 ul li a { color:#666;}

#bottom { width:100%; float:left; background:#acbaca; margin-top:10px;}
.bottom-box { width:33.3333%; *+ width:33%; float:left; padding-top:10px;}
.outer-bottom-box { margin-left:10px; -webkit-box-shadow: 6px 6px 6px #aabbc1; -moz-box-shadow: 6px 6px 6px #aabbc1; box-shadow: 6px 6px 6px #aabbc1;}
.outer-bottom-box h3 { color:#fff; font-weight:normal; height:19px; line-height:19px; border-bottom:1px solid #85add9; background:#E8E8E8;}
.outer-bottom-box h3 strong { display:block; float:left; padding:0 5px 0 8px; position:relative; font-weight:normal; background:url(../imgs/main/background/bg03_tit.png) repeat-x;}
.inter-bottom-box { position:relative; height:340px; *+height:340px; padding:15px; text-align:center; background:url(../imgs/main/background/bg033_box.png) right top no-repeat;}
.outer-bottom-box h3 strong em { width:2px; height:16px; position:absolute; left:2px; top:2px; display:block; background:#fff;}
.inter-bottom-box b.kt { position:absolute; left:0; top:0; display:block; height:340px; width:15px; background:url(../imgs/main/background/bg04_box.png) no-repeat;}

.secondbox { background:#fff;}
.secondbox li { display:block; float:left; width:33%;}
.secondbox li strong { text-align:center; white-space:nowrap; display:block; font-weight:normal; color:#274b6d;}
.secondbox_ li strong { letter-spacing:-1px}
/* Footer */
#footer { position:fixed; left:0; bottom:0; width:100%; line-height:21px; color:#fff; text-align:center; background:url("../imgs/main/background/bg01footer.png") repeat-x;}

/* Container  Left and Right commom*/
#left h2 { *+float:left; *+width:100%; position:relative; padding:0 0 0 13px; height:25px; line-height:25px; color:#2b5280; background:url("../imgs/main/background/bg01tit.png") repeat-x; -moz-border-radius: 4px 4px 0 0; -webkit-border-radius: 4px 4px 0 0; border-radius: 4px 4px 0 0;}
#right h2 { position:relative; padding:0 0 0 13px; height:25px; line-height:25px; color:#2b5280; background:url("../imgs/main/background/bg01tit.png") repeat-x; -moz-border-radius:4px 0 0 0; -webkit-border-radius:4px 0 0 0; border-radius:4px 0 0 0; }
#right h3, #left h3 { padding:3px 0 0 3px; height:28px; background:url("../imgs/main/background/bg02tit.png") repeat-x;}
#left h3.tit-other { height:29px; padding:0; background:url("../imgs/main/background/bg03tit.png") repeat-x #ebe9fd; -moz-border-radius: 0 6px 0 0; -webkit-border-radius: 0 6px 0 0; border-radius: 0 6px 0 0; color:#fff;}
#left h3.tit-other strong { display:block; float:left; height:29px; line-height:29px; width:48%; padding-left:1%; background:url(../imgs/main/background/bg04tit.png) no-repeat; text-align:left;}

/* Datalist */

.datalist { margin:0; padding:0; background:#f2ffff;}
.datalist thead th { line-height:25px; font-weight:normal; background:#3d9ddc; color:#fff; text-shadow:1px 1px 1px #2080bf; border-bottom:1px solid #317eb0;}
.datalist tbody td.th1 { text-align:left; font-size:12px; padding-left:10px; color:#000;border-bottom:1px solid #d4eeee; background-repeat:repeat; font-weight:normal; } 
.datalist tbody td.th1 a { color:#000; text-shadow:1px 1px 1px #fff;}
.datalist td { line-height:23px; padding:1px 0; border-bottom:1px solid #d4eeee; background-repeat:repeat; text-align:center; color:#333; font-size:11px; font-family:Arial, Helvetica, sans-serif;}
.datalist td.ar { text-align:right; padding-right:10px;}


.datalist tbody th.gary1, .datalist tbody td.gary1 { cursor:pointer!important; background-image: url(../imgs/main/background/bg02_td.png); background-repeat:repeat;}

/* Datalist1 */
.datalist1 { margin:0 ; padding:0; background:url(../imgs/main/background/bg01_table.png) left bottom no-repeat;}
.datalist1 td, .datalist1 th { line-height:23px; padding:1px 0; border-bottom:1px solid #dcdfe1;  text-align:center; font-weight:normal; text-align:left; padding-left:2px; color:#333; background:none;}
.datalist1 td.number-list span { font-family:Arial, Helvetica, sans-serif; font-size:10px; color:#fff; background:#b8b8b8; padding:1px 3px;}
.datalist1 td.number-list.n1 span { background:#ed0000;}
.datalist1 td.number-list.n2 span { background:#fabd02;}
.datalist1 td.number-list.n3 span { background:#e7e669;}
.datalist1 td.ar { text-align:right; padding-right:10px;}
.datalist1 td.al { text-align:left; padding-left:10px;}
.datalist1 td ul li { display:inline;}
.gary { background-color:#f7f7f7;}
.gary_ {background-color:#e8e8e8;}

.select-outer { position:absolute; z-index:999999999999; width:100%; height:27px; padding-top:3px; background:url(../imgs/main/background/bg01table.png) repeat-x;}
.select01 { float:left; margin-left:2px;}
.select01 input { margin-top:1px; width:68px; height:21px; border:0; margin:0; padding:0; padding-left:17px; line-height:21px; color:#a8a8a8; text-align:left; background:url("../imgs/main/block/select01.png") no-repeat; font-family:"宋体", "Arial Narrow", HELVETICA, Sans-serif; font-size:1em;}
.select01 input.input-w120 { width:103px; background:url("../imgs/main/block/select01_.png") no-repeat;}
.select01 ul { background:#fff; border:1px solid #efefef; border-top:0; margin:-3px 0 0 2px; padding-top:3px; box-shadow: 1px 3px 3px #333;}
.select01 ul li { line-height:1.8; font-weight:normal; padding-left:15px;}
.select01 ul li a { color:#666; text-decoration:none;}
.select01 .btn {display:inline-block; position:relative; top:-0px; *top:2px; background:url("../imgs/main/block/select02.png") left bottom no-repeat; width:20px; height:21px; text-indent:-5000px;}
.select01 .btn_ { display:inline-block; background:url("../imgs/main/block/select02_.png") left bottom no-repeat; width:20px; height:21px; text-indent:-5000px;}
.sample-box { margin-top:50px;}
/* Map */
.contents { position:relative; background:#fff; padding-top:2px;}
.contents h2 {position:relative;z-index:999998; color:#2b5280; padding:0 0 0 10px; height:27px; line-height:27px; background:url("../imgs/main/background/bg01tit.png") repeat;}

#windows { position:absolute; left:0; top:-7px; z-index:999997;}
.hide { position:absolute; left:12px; z-index:9; top:68px; background:#cbf3fd; width:340px; height:25px; color:#3667a1; -moz-border-radius: 4px 4px 0 0; -webkit-border-radius: 4px 4px 0 0; border-radius: 4px 4px 0 0; font-weight:bold; line-height:25px;padding-left:10px;}
.hide .map-windows-open {position:absolute; right:13px; top:5px;}
.map-windows { position:absolute; z-index:7; left:1px; top:68px; width:410px; padding:8px; background:url(../imgs/map/background/bg02map-windows.png) repeat; -moz-border-radius: 6px; -webkit-border-radius: 6px; border-radius: 6px; display:none; }
.map-windows h3 { height:25px; line-height:25px; padding-left:10px; color:#3667a1; -moz-border-radius: 4px 4px 0 0; -webkit-border-radius: 4px 4px 0 0; border-radius: 4px 4px 0 0; background:url("../imgs/main/background/bg01tit.png") repeat-x;}
.map-windows h3 a { position:absolute; right:18px; top:17px;} 
.map-windows h3 a.link01{ position:static;}
a.link01 { color:#f00;}
a.link01:hover { color:#000;}
.map-windows h4 { height:20px; line-height:20px; background:#4a7cb7; color:#fff; padding-left:10px; font-weight:normal;border-bottom: 1px solid #94a7be;}
.map-windows h4 span { float:right; margin:-20px 3px 0 0; font-weight:normal;}
.map-windows h4 span a { color:#fff; text-decoration:none; font-family:Arial, Helvetica, sans-serif; font-weight:bold; font-size:10px; opacity:0.7;}
.map-windows h4 span a:hover { opacity:1;}
.map-windows-outer { padding:0; background: #fff;}
.map-windows-outer .txt { line-height:1.5; padding:0 5px; background:#999; border-bottom: 1px solid #94a7be; color:#fff;}
.map-windows-outer .txt em { color:#fff; font-weight:bold;}
.map-windows-inter { background:#fff;}

.small-map { position:fixed; right:0; bottom:0; width:169px; height:316px; border:5px solid #ccc; background:url(../imgs/map/background/small-map.png) no-repeat;}
.small-map ul {display:block; height:100%;}
.small-map ul li a{ display:block; height:79px;}
.small-map ul li a:hover { background:url(../imgs/map/background/small-map-black.png) repeat;}
.small-map-c { overflow:hidden; height:2226px; position:absolute; left:0; top:0; width:100%;}
.small-map-c li { height:556px;}

